  <PropertyGroup>
    <PreBuildEvent>REM build chromium &amp; cef
REM devenv "$(CHROMIUM_SRC)\cef\cef.sln" /Build $(Configuration)

REM copy locales &amp; pack files
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\*.pak" "$(TargetDir)" /s /i /d /y
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\locales\*" "$(TargetDir)\locales" /s /i /d /y

REM copy libraries &amp; debug symbols
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\libcef.*" "$(TargetDir)" /s /i /d /y
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\icudt.dll" "$(TargetDir)" /s /i /d /y
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\libEGL.dll" "$(TargetDir)" /s /i /d /y
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\libEGLESv2.dll" "$(TargetDir)" /s /i /d /y

REM copy video &amp; audio codecs
xcopy "$(CHROMIUM_SRC)\build\$(Configuration)\av*.dll" "$(TargetDir)" /s /i /d /y</PreBuildEvent>
  </PropertyGroup>
